(set-option :produce-models true)
(set-option :strings-fmf true)

(set-logic QF_S)

(declare-const r String)
(declare-const i Int)

;; Invariant :: (= i (str.len r))

(define-fun post_check () Bool
            (=> (and (= i (str.len r)) (not (>= (str.indexOf r "aa") 0)))
                (and (= i (str.len r)) (not (str.contains r "aa")))))

(assert (not post_check))
(check-sat)

(get-value (r i))
