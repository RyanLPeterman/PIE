(declare-const r String)
(declare-const i Int)

;; Invariant :: (> (str.len r) i) && (i >= 0) && (str.contains r "a")

(define-fun ind_check () Bool
            (=> (and (> (Length r) i) (>= i 0) (Contains r "a"))
                (and (> (Length (Replace r "a" "aa")) (+ i 1)) (>= (+ i 1) 0) (Contains r "a"))))

(assert (not ind_check))
(check-sat)
