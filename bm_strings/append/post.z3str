(declare-const s0 String)
(declare-const s1 String)
(declare-const r String)
(declare-const i Int)

;; Invariant :: r = s0 ++ s1[0:i]

(define-fun post_check () Bool
            (=> (and (not (< i (Length s1))) (= r (Concat s0 (Substring s1 0 i))))
                (= r (Concat s0 s1))))

(assert (not post_check))
(check-sat)
